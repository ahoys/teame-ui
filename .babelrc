{
  "presets": [
    "@babel/preset-react",
    "@babel/preset-typescript",
    "@babel/preset-env"
  ],
  "env": {
    "production": {
      "plugins": [
        "@babel/plugin-syntax-dynamic-import",
        "emotion"
      ]
    },
    "development": {
      "plugins": [
        "@babel/plugin-syntax-dynamic-import",
        ["emotion", { "sourceMap": true }]
      ]
    }
  }
}
